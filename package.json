{
  "name": "copified-engine",
  "version": "0.1.0",
  "description": "Copified Engine",
  "main": "dist/index.js",
  "license": "ISC",
  "scripts": {
    "copy-assets": "yarn cpy '**/*' '!**/*.ts' ../dist/ --cwd=src/ --no-overwrite --parents",
    "postinstall": "yarn tsc && yarn copy-assets",
    "start": "node dist/index.js",
    "run-ts-script": "tsnd --respawn ",
    "dev": "yarn run-ts-script src/index.ts",
    "test": "yarn jest",
    "test:watch": "yarn jest --watch",
    "eslint:fix": "eslint --cache --fix",
    "format:fix": "prettier --write"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.0",
    "@types/fs-extra": "^9.0.2",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.11.2",
    "@types/puppeteer": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "cpy-cli": "^3.1.1",
    "del-cli": "^3.0.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": ">=4",
    "jest": "^26.4.0",
    "lint-staged": ">=10",
    "nodemon": "^2.0.4",
    "prettier": "2.0.5",
    "ts-jest": "^26.2.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,html,css}": [
      "yarn eslint:fix",
      "yarn format:fix"
    ]
  },
  "dependencies": {
    "apollo-server-express": "^2.18.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.12.2",
    "connect-redis": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "fs-extra": "^9.0.1",
    "graphql": "^15.3.0",
    "ioredis": "^4.17.3",
    "pg": "^8.3.3",
    "puppeteer": "^5.3.1",
    "puppeteer-extra": "^3.1.15",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.0.0",
    "typeorm": "^0.2.26"
  }
}